<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="/stylesheets/forMain.css" />
  </head>
  <body>
    <div class="main-grid-container">
      <header>

        <img class="welcome-img" src="/images/put_out_the_picture.jpg" />
        <div class="indroduction-img-container">
          <p class="bottom-text">잠시 빌리려 왔습니다</p>
        </div>
      </header>

      <main>
        <% items.forEach(item => { %>
          <div class="itemDetail-card">
            <h3><%= item.itemName %></h3>
            <ul>
              <li>
                <img src="<%= item.img %>" alt="<%= item.itemName %>" />
              </li>
            </ul>
          </div>
        <% }) %>
      </main>
    </div>
  </body>
</html>